    "grunt-browserify": "~1.2.11",
    "grunt-contrib-uglify": "*",
    "grunt-text-replace": "*",
    "grunt-jsbeautifier": "*",
    "grunt-contrib-jshint": "*",
    "grunt-jsdoc": "*",
    "grunt-mocha-test": "~0.8.1",
